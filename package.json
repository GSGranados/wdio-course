{
  "name": "wdio-course",
  "version": "1.0.0",
  "description": "wdio end to end testing project",
  "main": "index.js",
  "scripts": {
    "test:debug": "set DEBUG=Y  && npx wdio config/wdio.test.conf.ts --cucumberOpts.tagExpression='@demo'",
    "test:demo": "set DEBUG=N && npx wdio config/wdio.test.conf.ts --cucumberOpts.tagExpression='@demo'",
    "test:smoke": "set DEBUG=N  && npx wdio config/wdio.test.conf.ts --cucumberOpts.tagExpression='@smoke'",
    "uat:demo": "npx wdio config/wdio.uat.conf.ts --cucumberOpts.tagExpression='@demo'",
    "uat:smoke": "npx wdio config/wdio.uat.conf.ts --cucumberOpts.tagExpression='@smoke'"
  },
  "keywords": [
    "wdio",
    "cucumber",
    "e2e",
    "automation"
  ],
  "author": "Steven Granados (GSGranados)",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/supertest": "^2.0.11",
    "@wdio/allure-reporter": "^7.16.6",
    "@wdio/cli": "^7.16.7",
    "@wdio/cucumber-framework": "^7.16.6",
    "@wdio/local-runner": "^7.16.7",
    "@wdio/spec-reporter": "^7.16.4",
    "chai": "^4.3.4",
    "chromedriver": "^95.0.0",
    "dotenv": "^10.0.0",
    "geckodriver": "^3.0.1",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "wdio-chromedriver-service": "^7.2.2",
    "wdio-geckodriver-service": "^2.1.0",
    "winston": "^3.3.3"
  }
}
